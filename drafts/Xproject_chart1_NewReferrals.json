 {
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

 "title": {
      "text": "New Referrals",
      "subtitle": "Number of patients who started new care pathways",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
 },

"data": { 
    "url": "https://raw.githubusercontent.com/rosieneville/rosieneville.github.io/main/newPeriodCommissioner_total.csv}",
    "format": {"type": "csv"}
},

"mark": {"type": "line"},
    
"encoding": {
    "x": {
        "field": "MonthCode",
        "type": "quantitative",
        "title": null,
        "axis": {"grid": false}
        },

    "y": {
        "field": "NewReferrals",
        "type": "quantitative",
        "title": null,
        "axis": {"grid": false}
        },
    
    "color":{
        "field": "Year",
        "type": "ordinal",
        "scale": {"scheme": "set1"},
        "legend": {"orient": "top-right"}
        }
    }

}
